import java.util.*;
public class Main
{
	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int n =sc.nextInt();
	int count=0;
	List<Integer> l1=new LinkedList<>();
	for(int i=0;i<n;i++)
	{
	    l1.add(sc.nextInt());
	}
	int key=sc.nextInt();
	for(int i=0;i<n;i++)
	{
	    if(l1.get(i)==key)
	    count++;
	}
	System.out.println(count);
	}
}
